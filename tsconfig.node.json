{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "lib": ["DOM", "ES2020"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmitOnError": true,
        "noImplicitAny": false,
        "skipLibCheck": true,
        "target": "ES2020"
    },
    "extends": "tsconfig-vuepress/base.json",
    "include": ["src/**/*.ts"]
}
